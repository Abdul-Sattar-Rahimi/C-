using System;

class Program
{
    static void Main()
    {

        int numbers = { 1, 2, 3, 4, 5 };


        Console.WriteLine("عناصر آرایه:");
        foreach (int number in numbers)
        {
            Console.WriteLine(number);
        }

        int sum = 0;
        foreach (int number in numbers)
        {
            sum += number;
        }

        Console.WriteLine("مجموع عناصر آرایه: " + sum);
    }
}

